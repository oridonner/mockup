  "main": "server.js",
  "scripts": {
    "build": "webpack",
    "start": "node ./dist/bundle.min.js",
    "dev": "yarn build && concurrently \"yarn watch:build\" \"yarn watch:start\"",
    "watch:build": "webpack --watch",
    "watch:start": "nodemon --legacy-watch \"./dist/bundle.min.js\" --watch \"./dist\" ",
    "clean": "rimraf ./dist",
    "clean:logs": "rimraf ./logs",
    "clean:mods": "rimraf ./node_modules",
    "lint": "npx eslint . --ext .ts"
  },
  "engines": {
    "npm": ">=6.14.0",
    "node": ">=14.17.0",
    "yarn": ">=1.22.10"
  },
  "dependencies": {
    "await-to-js": "^3.0.0",
    "casual": "^1.6.2",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "http": "^0.0.1-security",
    "http-proxy-middleware": "^2.0.3",
    "is-docker": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.3"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.13",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "nodemon-webpack-plugin": "^4.7.1",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "tsc": "^2.0.3",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0"
  }
}
